{
  "version": 2,
  "name": "my-news-be",
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "functions": {
    "src/common/constants.js": {
      "includeFiles": "data/providers.json"
    }
  }
}